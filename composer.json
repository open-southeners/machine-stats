{
    "name": "open-southeners/machine-stats",
    "description": "A platform agnostic machine stats generator (PHP 8.1+). Usage and totals for CPU, disk, memory...",
    "license": "MIT",
    "keywords": [
        "open-southeners",
        "machine stats",
        "linux",
        "windows",
        "macos",
        "resources usage",
        "memory usage",
        "disk usage",
        "cpu usage"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "me@d8vjork.com",
            "homepage": "https://d8vjork.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "open-southeners/byte-unit-converter": "^1.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\MachineStats\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\MachineStats\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}